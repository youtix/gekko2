{
  "name": "gekko2",
  "version": "3.0.0",
  "descripton": "A modular crypto trading bot framework supporting backtesting, real-time trading, and custom strategies.",
  "keywords": [
    "trading",
    "bot",
    "crypto",
    "cryptocurrency",
    "trading-bot",
    "backtest",
    "backtesting",
    "real-time",
    "strategies",
    "indicators",
    "automated-trading",
    "technical-analysis",
    "gekko",
    "gekko2",
    "exchange",
    "finance",
    "market-data",
    "trading-strategy",
    "trading-algorithms",
    "plugin-architecture"
  ],
  "author": "youtix",
  "license": "MIT",
  "main": "gekko2.js",
  "type": "module",
  "scripts": {
    "build:exec": "bun build src/gekko2.ts --compile --outfile dist/gekko2",
    "build:js": "bun build ./src/gekko2.ts --outdir ./dist --target bun --minify --sourcemap",
    "commit:check": "commitlint --from=HEAD~10",
    "dev": "bun ./src/gekko2.ts",
    "format": "bun prettier ./src --write",
    "format:check": "prettier --check \"src/**/*.{ts,js,json,md}\"",
    "lint": "bun eslint ./src --fix",
    "lint:check": "bun eslint ./src",
    "preinstall": "git update-index --assume-unchanged ./src/strategies/custom/index.ts",
    "prepare": "husky",
    "release": "bun semantic-release",
    "start": "bun dist/gekko2.js",
    "bench": "vitest --run bench",
    "bench:watch": "vitest bench",
    "test": "vitest --run",
    "test:coverage": "vitest run --coverage",
    "test:watch": "vitest",
    "test:e2e": "bun test test/e2e",
    "type:check": "bun tsc --noEmit"
  },
  "devDependencies": {
    "@commitlint/cli": "^20.4.2",
    "@commitlint/config-conventional": "^20.4.2",
    "@eslint/js": "^10.0.1",
    "@semantic-release/changelog": "^6.0.3",
    "@semantic-release/commit-analyzer": "^13.0.1",
    "@semantic-release/git": "^10.0.1",
    "@semantic-release/github": "^12.0.6",
    "@semantic-release/release-notes-generator": "^14.1.0",
    "@types/bun": "^1.3.9",
    "@types/js-yaml": "^4.0.9",
    "@types/lodash-es": "^4.17.12",
    "@typescript-eslint/parser": "^8.56.0",
    "@vitest/coverage-v8": "^4.0.18",
    "eslint": "^10.0.1",
    "eslint-import-resolver-typescript": "^4.4.4",
    "eslint-plugin-import": "^2.32.0",
    "globals": "^17.3.0",
    "husky": "^9.1.7",
    "lint-staged": "^16.2.7",
    "prettier": "^3.8.1",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.56.0",
    "vitest": "^4.0.18"
  },
  "dependencies": {
    "@date-fns/tz": "^1.4.1",
    "ccxt": "^4.5.39",
    "date-fns": "^4.1.0",
    "https-proxy-agent": "^7.0.6",
    "js-yaml": "^4.1.1",
    "json5": "^2.2.3",
    "lodash-es": "^4.17.23",
    "protobufjs": "^8.0.0",
    "socks-proxy-agent": "^8.0.5",
    "winston": "^3.19.0",
    "zod": "^4.3.6"
  }
}
