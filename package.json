{
  "name": "gekko2",
  "version": "0.1.0",
  "descripton": "A modular crypto trading bot framework supporting backtesting, real-time trading, and custom strategies.",
  "keywords": [
    "trading",
    "bot",
    "crypto",
    "cryptocurrency",
    "trading-bot",
    "backtest",
    "backtesting",
    "real-time",
    "strategies",
    "indicators",
    "automated-trading",
    "technical-analysis",
    "gekko",
    "gekko2",
    "exchange",
    "finance",
    "market-data",
    "trading-strategy",
    "trading-algorithms",
    "plugin-architecture"
  ],
  "author": "youtix",
  "license": "MIT",
  "main": "gekko.js",
  "type": "module",
  "scripts": {
    "preinstall": "git update-index --assume-unchanged ./src/strategies/custom/index.ts",
    "build": "bun build ./src/gekko.ts --outdir ./dist --target bun --minify --sourcemap",
    "dev": "bun ./src/gekko.ts",
    "lint": "bun eslint ./src --fix",
    "lint:check": "bun eslint ./src",
    "format": "bun prettier ./src --write",
    "format:check": "prettier --check \"src/**/*.{ts,js,json,md}\"",
    "prepare": "husky",
    "start": "bun dist/gekko.js",
    "test": "vitest --run",
    "test:coverage": "vitest run --coverage",
    "test:watch": "vitest",
    "type:check": "bun tsc --noEmit"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.8.1",
    "@commitlint/config-conventional": "^19.8.1",
    "@eslint/js": "^9.26.0",
    "@types/big.js": "^6.2.2",
    "@types/bun": "^1.2.12",
    "@types/js-yaml": "^4.0.9",
    "@types/lodash-es": "^4.17.12",
    "@typescript-eslint/parser": "^8.32.0",
    "@vitest/coverage-v8": "^3.1.3",
    "eslint": "^9.26.0",
    "eslint-import-resolver-typescript": "^4.3.4",
    "eslint-plugin-import": "^2.31.0",
    "globals": "^16.1.0",
    "husky": "^9.1.7",
    "lint-staged": "^15.5.2",
    "prettier": "^3.5.3",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.32.0",
    "vitest": "^3.1.3"
  },
  "dependencies": {
    "@date-fns/tz": "^1.2.0",
    "big.js": "^7.0.1",
    "ccxt": "^4.4.78",
    "date-fns": "^4.1.0",
    "inquirer": "^12.6.0",
    "js-yaml": "^4.1.0",
    "json5": "^2.2.3",
    "lodash-es": "^4.17.21",
    "winston": "^3.17.0",
    "yup": "^1.6.1"
  }
}
